{
    "name": "ed.sukharev/re-captcha-validator",
    "type": "library",
    "description": "Google reCaptcha FormType and Validator for Symfony forms.",
    "keywords": ["reCaptcha", "captcha", "security", "symfony", "form", "validator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Pokamestov",
            "email": "dario_swain@yahoo.com",
            "homepage": "http://github.com/DarioSwain",
            "role": "Developer"
        },
        {
            "name": "Eduard Sukharev",
            "email": "sukharev.eh@gmail.com",
            "homepage": "http://github.com/eduard-sukharev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": {
            "DS\\Component\\ReCaptchaValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DS\\Component\\ReCaptchaValidator\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v1.0.0-dev"
        }
    }
